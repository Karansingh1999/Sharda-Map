<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.examples.chooseyouroption.AdmissionCell1">



    <VideoView
        android:layout_width="wrap_content"
        android:layout_alignParentTop="true"
        android:id="@+id/videoView"
        android:layout_height="300dp" />


    <ListView
        android:layout_width="match_parent"
        android:id="@+id/listview_admission"
        android:layout_below="@id/videoView"
        android:entries="@array/admission"
        android:layout_above="@id/admissionlink_button"
        android:layout_height="wrap_content">


    </ListView>






    <Button
        android:layout_width="match_parent"
        android:id="@+id/admissionlink_button"
        android:layout_alignParentBottom="true"
        android:autoLink="all"
        android:clickable="true"
        android:background="@drawable/button_design6"
        android:text="www.sharda.ac.in"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_height="50dp" />



</RelativeLayout>